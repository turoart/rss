{
  "manifest_version": 2,

  "name": "add to rss",
  "description": "Add current url to rss tool https://github.com/Sufrostico/rss",
  "version": "1.0",

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Add url to rss"
  },

  "permissions": [
    "tabs", "http://*/*", "https://*/*" 
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
